<?xml version="1.0"?>
<odoo>
    <record id="module_na_linkdanea_category" model="ir.module.category">
        <field name="name">Sync Danea</field>
    </record>
    <record id="module_na_linkdanea_group_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_na_linkdanea_category"/>
        <field name="implied_ids" eval="[(4,ref('base.group_user'))]"/>
    </record>
    <record id="module_na_linkdanea_group_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="module_na_linkdanea_category"/>
        <field name="implied_ids" eval="[(4,ref('module_na_linkdanea_group_user'))]"/>
        <field name="users" eval="[(4,ref('base.user_root')),(4,ref('base.user_admin'))]"/>
    </record>
</odoo>